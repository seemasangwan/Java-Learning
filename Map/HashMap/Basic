
import java.util.Scanner;
import java.util.HashMap;
import java.util.Map;
class Main {
    public static void main(String[] args) {
       HashMap<String , String>map=new HashMap<String,String>();
       // how to insert values in map
       map.put("key1","value1");
       map.put("key2","value2");
       // how to print map data
       System.out.println(map);
       
       // how to search partiluar value its in map or not 
       // containskey --> true if exist . false if does Not exist
       System.out.println(map.containsKey("key1"));
       System.out.println(map.containsKey("key3"));

       // how to get value of praticluar key from map 
       // get(key) ---> value if key exist
       // return null if key does not exist
       System.out.println(map.get("key1"));
       System.out.println(map.get("key3"));

       
       // how to travers in map 
       //1 using entrySet
        System.out.println("Traverse in map using entrySet()");
       for(Map.Entry<String,String>ele:map.entrySet())
       {
            System.out.println("element is "+ele.getKey()+" : "+ele.getValue());
       }
       //2. usingkeyset
     System.out.println("Traverse in map using keySet()");
    
     for(String key:map.keySet())
       {
            System.out.println("element is "+key+" : "+map.get(key));
 
      }
      
       // 3. traverse using value
        System.out.println("Traverse in map using values()");
   for(String val:map.values())
   {
       System.out.println(val);
   }
     
     // how to remove element 
    System.out.println("how to remove element ");
    System.out.println(map.remove("key2"));
   System.out.println(map.remove("key3"));
   System.out.println(map);
   
  
   // how to get map size 
    System.out.println("how to get map size ");
   System.out.println(map.size());
   
   // how to remove all element
    System.out.println("how to remove all element ");

   map.clear();
      System.out.println(map);

    }
}



/* ========================OutPut==============================


{key1=value1, key2=value2}
true
false
value1
null
Traverse in map using entrySet()
element is key1 : value1
element is key2 : value2
Traverse in map using keySet()
element is key1 : value1
element is key2 : value2
Traverse in map using values()
value1
value2
how to remove element 
value2
null
{key1=value1}
how to get map size 
1
how to remove all element 
{}

*/
